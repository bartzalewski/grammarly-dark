{
  "manifest_version": 3,
  "name": "Grammarly Dark",
  "description": "A dark theme for Grammarly documents.",
  "version": "1.0.13",
  "content_scripts": [
    {
      "matches": [
        "https://app.grammarly.com/*",
        "https://account.grammarly.com/*"
      ],
      "css": ["index.css"]
    }
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
